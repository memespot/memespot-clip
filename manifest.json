{
  "manifest_version": 2,
  "name": "memespot-clip",
  "version": "0.0.1",
  "description": "memespot-clip",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["asset/js/jquery-2.1.4.min.js","asset/js/jquery-ui.min.js","contentscript.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "<all_urls>"
  ],
  "icons": {
    "128": "asset/images/pickr_icon_128.png",
    "19": "asset/images/pickr_chrome_icon.png",
    "48": "asset/images/pickr_icon_48.png"
  }
}
