{
  "manifest_version": 2,
  "name": "memespot-clip",
  "version": "0.0.1",
  "description": "memespot-clip",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentscript.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "128": "images/pickr_icon_128.png",
    "19": "images/pickr_chrome_icon.png",
    "48": "images/pickr_icon_48.png"
  }
}
